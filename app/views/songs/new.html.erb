<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre_name %>
  <%= f.collection_select :genre, :name, Genre.all, :genre_name, :genre_name %>
  <br />
  <label form="song_notes_1">Note 1:</label>
  <input id="song_notes_1" type="text" name="song[note_contents][]" >
  <br />
  <label form="song_notes_2">Note 2:</label>
  <input id="song_notes_2" type="text" name="song[note_contents][]" >
  <br />
  <%= f.submit %>
<% end %>


